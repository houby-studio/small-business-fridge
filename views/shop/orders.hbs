<div class="row my-3">
    <div class="col">
        <div class="table-responsive">
            <table id="table-orders" class="table">
                <thead>
                    <th scope="col">Datum</th>
                    <th scope="col">Produkt</th>
                    <th scope="col">Cena</th>
                    <th scope="col">Dodavatel</th>
                    <th scope="col">Způsob</th>
                    <th scope="col">Fakturováno</th>
                    <th scope="col">Zaplaceno</th>
                </thead>
                <tbody>
                    {{# each orders.results }}
                        <tr>
                            <td>
                                <span class="d-none">{{ this.order_date }}</span>{{ this.order_date_format }}
                            </td>
                            <td>
                                {{ this.productInfo.displayName}}
                            </td>
                            <td>
                                {{ this.deliveryInfo.price}}Kč
                            </td>
                            <td>
                                {{ this.supplierInfo.displayName }}
                            </td>
                            <td>
                                {{#if this.keypadOrder }}
                                    Klávesnice
                                {{else}}
                                    E-Shop
                                {{/if}}
                            </td>
                            <td>
                                {{#if this.invoice }}
                                    Ano
                                {{else}}
                                    Ne
                                {{/if}}
                            </td>
                            <td>
                                Ne
                            </td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>
<script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0/dist/Chart.min.js"></script>
<script src="https://cdn.jsdelivr.net/gh/emn178/chartjs-plugin-labels/src/chartjs-plugin-labels.js"></script>
<script src="/javascripts/orders.js"></script>

